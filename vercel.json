{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/f/(?<videoID>[^/]+)",
      "dest": "/public/index.html?videoID=$videoID"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}